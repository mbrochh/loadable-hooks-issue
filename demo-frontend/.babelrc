{
    "env": {
        "production": {
            "presets": [
                "razzle/babel",
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "@loadable/babel-plugin",
                [
                    "emotion",
                    {
                        "hoist": true
                    }
                ]
            ]
        },
        "development": {
            "presets": [
                "razzle/babel",
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "@loadable/babel-plugin",
                [
                    "emotion",
                    {
                        "sourceMap": true,
                        "autoLabel": true
                    }
                ]
            ]
        }
    }
}